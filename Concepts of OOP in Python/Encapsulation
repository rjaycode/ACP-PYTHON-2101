class Person:
    def __init__(self):
        self.__age = 0

    @property
    def age(self):
        return self.__age

    @age.setter
    def age(self, value):
        if isinstance(value, int) and value > 0:
            self.__age = value
        else:
            print("Invalid age. Must be a positive integer.")

p = Person()
p.age = 20
print(p.age)
